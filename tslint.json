{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {
        "no-console": false,
        "no-unused-expression": true
    },
    "rules": {
        "quotemark": [true, "single"],
        "member-access": [false],
        "ordered-imports": [false],
        "max-line-length": [true, 150],
        "member-ordering": [false],
        "interface-name": [false],
        "arrow-parens": false
    },
    "rulesDirectory": []
}

